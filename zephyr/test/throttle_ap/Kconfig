source "Kconfig.zephyr"

config AP_POWER_CONTROL
	bool "Allows the test to mock various chipset functions"
	default y

config TEST_INTERRUPT
	bool "Enable testing of prochot interrupts"
	default n

config TEST_THROTTLE_AP
	bool "Enable testing of AP throttling functions"
	default n
