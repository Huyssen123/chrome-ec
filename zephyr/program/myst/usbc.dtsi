/ {
	usbc {
		#address-cells = <1>;
		#size-cells = <0>;

		usbc_port0: port0@0 {
			compatible = "named-usbc-port";
			reg = <0>;
			tcpc = <&tcpc_port0>;
			chg = <&charger>;
			ppc = <&ppc_port0>;
			usb-mux-chain-0 {
				compatible = "cros-ec,usb-mux-chain";
				usb-muxes = <&amd_fp8_port0>;
			};
		};

		usbc_port1: port1@1 {
			compatible = "named-usbc-port";
			reg = <1>;
			usb-mux-chain-1-anx {
				compatible = "cros-ec,usb-mux-chain";
				usb-muxes = <&amd_fp8_port1>;
			};
		};
	};
};
