			#gpio-cells = <0>;
			gpios = <&gpiob 1 GPIO_INPUT>;
			enum-name = "GPIO_USB_C0_RT_INT_ODL";
		};
		usb_c2_rt_int_odl: usb_c2_rt_int_odl {
			#gpio-cells = <0>;
			gpios = <&gpio4 1 GPIO_INPUT>;
			enum-name = "GPIO_USB_C2_RT_INT_ODL";
