			gpios = <&gpiob 1 GPIO_INPUT>;
			enum-name = "GPIO_USB_C0_INT_ODL";
		};
		usb_c2_int_odl {
			gpios = <&gpio4 1 GPIO_INPUT>;
			enum-name = "GPIO_USB_C2_INT_ODL";
		};
