/ {
	displight {
		compatible = "cros-ec,displight";
		pwms = <&pwm5 0 PWM_POLARITY_NORMAL>;
		frequency = <4800>;
		generic-pwm-channel = <1>;
	};
};

&pwm5 {
	status = "okay";
};
