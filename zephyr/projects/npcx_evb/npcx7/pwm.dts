/ {
	named-pwms {
		compatible = "named-pwms";

		fan {
			pwms = <&pwm0 0 0>;
			label = "FAN";
			frequency = <25000>;
		};
		kblight {
			pwms = <&pwm2 0 0>;
			label = "KBLIGHT";
			frequency = <10000>;
		};
	};
};

/* fan */
&pwm0 {
	status = "okay";
	drive-open-drain;
};

/* kblight */
&pwm2 {
	status = "okay";
};
