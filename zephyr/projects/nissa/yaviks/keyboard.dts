/ {
	kblight {
		compatible = "cros-ec,kblight-pwm";
		pwms = <&pwm0 0 PWM_KHZ(10) PWM_POLARITY_NORMAL>;
	};
};

&pwm0 {
	status = "okay";
	prescaler-cx = <PWM_PRESCALER_C4>;
	pinctrl-0 = <&pwm0_gpa0_default>;
	pinctrl-names = "default";
};
