/ {
	usbc {
		port0: usbc-port0 {
			bc12 {
				compatible = "pericom,pi3usb9201";
				port = <&i2c_usb_c0>;
				irq = <&int_usb_c0_bc12>;
			};
		};

		port1: usbc-port1 {
			bc12 {
				compatible = "pericom,pi3usb9201";
				port = <&i2c_usb_c1>;
				irq = <&int_usb_c1_mix_bc12>;
			};
		};
	};
};
